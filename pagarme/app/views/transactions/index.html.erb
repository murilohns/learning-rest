<p id="notice"><%= notice %></p>

<h1>Transactions</h1>

<table>
  <thead>
    <tr>
      <th>Card number</th>
      <th>Card holder name</th>
      <th>Card expiration month</th>
      <th>Card expiration year</th>
      <th>Card cvv</th>
      <th>Card hash</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transactions.each do |transaction| %>
      <tr>
        <td><%= transaction.card_number %></td>
        <td><%= transaction.card_holder_name %></td>
        <td><%= transaction.card_expiration_month %></td>
        <td><%= transaction.card_expiration_year %></td>
        <td><%= transaction.card_cvv %></td>
        <td><%= transaction.card_hash %></td>
        <td><%= link_to 'Show', transaction %></td>
        <td><%= link_to 'Edit', edit_transaction_path(transaction) %></td>
        <td><%= link_to 'Destroy', transaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transaction', new_transaction_path %>
